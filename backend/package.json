{
  "name": "skdsm",
  "version": "1.0.0",
  "description": "REST server for Steel Kiwi Dev Skill Managment System",
  "main": "app.js",
  "scripts": {
    "start": "nodemon src/app.js"
  },
  "repository": {
    "type": "git",
    "url": "https://git.steelkiwi.com/changli/skd-sm.git"
  },
  "author": "Alexander Changli",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.2",
    "connect-mongo": "^1.3.2",
    "express": "^4.14.1",
    "express-session": "^1.15.1",
    "moment": "^2.17.1",
    "mongoose": "^4.8.5",
    "nodemon": "^1.11.0",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.3"
  },
  "devDependencies": {
    "eslint-config-xo-space": "^0.15.0",
    "xo": "^0.17.1"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6
    },
    "rules": {
      "indent": [
        "warn",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "semi": [
        "error",
        "always"
      ],
      "no-warning-comments": 0,
      "camelcase": 0,
      "object-curly-spacing": 0
    },
    "extends": [
      "xo-space/esnext"
    ]
  }
}
